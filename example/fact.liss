(fn _benchmark [fun]
    (let _start (time_ms))
    (let _res (fun))
    (let _end (time_ms))
    (print "Time taken (msec): ")
    (print (- _end _start))
    (print "\n\n")
    _res
)

(fn fact [n]
    (cond (= n 0)
        1
        ( * n (fact (- n 1)))
    )
)

(let _res (_benchmark (fn []
    (fact 25)
)))
(print _res)
(print "\n")
