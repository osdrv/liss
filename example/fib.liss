(fn _benchmark [fun]
    (let _start (time_ms))
    (let _res (fun))
    (let _end (time_ms))
    (print "Time taken (msec): ")
    (println (- _end _start))
    _res
)

(fn fib [n]
    (cond (= n 0)
        0
        (cond (= n 1)
            1
            (+(fib (- n 1)) (fib (- n 2)))
        )
    )
)

(let res (_benchmark (fn []
    (fib 30)
)))
(println res)
